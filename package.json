{
  "name": "strapi-plugin-shopify",
  "version": "0.0.5",
  "description": "Integrate Shopify in your Strapi application.",
  "license": "MIT",
  "files": [
    "admin",
    "server",
    "strapi-admin.js",
    "strapi-server.js"
  ],
  "strapi": {
    "name": "shopify",
    "displayName": "Shopify",
    "description": "Integrate Shopify in your Strapi application.",
    "kind": "plugin"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shop3/strapi-plugin-shopify.git"
  },
  "scripts": {
    "release": "standard-version",
    "lint": "eslint --fix .",
    "lint:staged": "eslint --fix",
    "format": "prettier --write .",
    "husky:install": "husky install",
    "pre-commit": "lint-staged",
    "commit-msg": "commitlint --edit"
  },
  "dependencies": {
    "@shopify/shopify-api": "^5.2.0"
  },
  "peerDependencies": {
    "@strapi/design-system": "^1.2.5",
    "@strapi/helper-plugin": "^4.4.5",
    "@strapi/strapi": "^4.4.5",
    "@strapi/utils": "^4.4.5",
    "axios": "^0.24.0",
    "koa-body": "^4.2.0",
    "prop-types": "^15.8.1",
    "qs": "^6.11.0",
    "react": "^17.0.2",
    "react-intl": "^5.25.1",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@babel/preset-react": "^7.18.6",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "standard-version": "^9.5.0"
  },
  "engines": {
    "node": ">=12.x.x <=16.x.x",
    "npm": ">=6.0.0"
  },
  "lint-staged": {
    "*.(ts|tsx|js)": [
      "npm run lint:staged"
    ]
  }
}
